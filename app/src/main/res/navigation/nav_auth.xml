<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/nav_auth"
  app:startDestination="@id/signInFragment">
  
  <fragment
    android:id="@+id/signInFragment"
    android:name="com.example.restgarden.ui.screen.SignInFragment"
    android:label="fragment_sign_in"
    tools:layout="@layout/fragment_sign_in" />
  <fragment
    android:id="@+id/registerFragment"
    android:name="com.example.restgarden.ui.screen.RegisterFragment"
    android:label="fragment_register"
    tools:layout="@layout/fragment_register" />
  <action
    android:id="@+id/action_global_signInFragment_to_registerFragment"
    app:destination="@id/registerFragment" />
  <action
    android:id="@+id/action_global_registerFragment_to_signInFragment"
    app:destination="@id/signInFragment" />
</navigation>
